{
  "name": "node-raspistill",
  "version": "1.0.4",
  "description": "Node wrapper for taking photos using Raspberry Pi raspistill app.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "typings": "build/index",
  "scripts": {
    "build": "rm -rf ./build && tsc",
    "watch": "tsc --watch",
    "test": "jest --coverage --runInBand --forceExit",
    "lint": "./node_modules/.bin/eslint './src/**/*.ts' --fix",
    "ci-lint": "./node_modules/.bin/eslint './src/**/*.ts'",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlukanin/node-raspistill.git"
  },
  "keywords": [
    "raspberry",
    "camera",
    "raspistill",
    "typescript",
    "iot"
  ],
  "author": "dylukanin@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dlukanin/node-raspistill/issues"
  },
  "homepage": "https://github.com/dlukanin/node-raspistill#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.15",
    "@types/rmfr": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.29.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^33.0.1",
    "fs-promise": "^2.0.3",
    "jest": "^26.6.3",
    "js-image-generator": "^1.0.3",
    "nock-exec": "^0.1.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@dlukanin/cla-mapper": "^1.1.0",
    "image-type": "4.1.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
